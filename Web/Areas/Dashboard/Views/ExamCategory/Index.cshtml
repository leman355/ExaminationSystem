@model IEnumerable<ExamCategory>
<a href="/dashboard/ExamCategory/create" class="btn btn-outline-primary mb-3">Create</a>


<div class="card shadow mb-4">
    <div class="card-header py-3">
        <h6 class="m-0 font-weight-bold text-primary">Exam Category datatable</h6>
    </div>
    <div class="card-body">
        <div class="table-responsive">
            <table class="table table-bordered" id="dataTable" width="100%" cellspacing="0">
                <thead>
                    <tr>
                        <th>Id</th>
                        <th>Category Name</th>
                        <th></th>
                    </tr>
                </thead>
                <tfoot>
                    <tr>
                        <th>Id</th>
                        <th>Category Name</th>
                        <th></th>
                    </tr>
                </tfoot>
                <tbody>
                    @foreach (var examCategory in Model)
                    {
                        <tr>
                            <td>@examCategory.Id</td>
                            <td>@examCategory.CategoryName</td>

                            <td class="col-2">
                                <a href="/dashboard/examCategory/edit/@examCategory.Id" class="btn btn-outline-warning">Edit</a>
                                <a href="/dashboard/examCategory/delete/@examCategory.Id" class="btn btn-outline-danger">Delete</a>
                            </td>
                        </tr>
                    }
                </tbody>
            </table>
        </div>
    </div>
</div>